NUM: DW 153
COPY: DW 0
SUM: DW 0

start:
 MOV DI, word NUM
 MOV  word COPY, DI

HESAPLA:
 MOV AX, DI
 MOV DX, 0
 MOV BX, 10
 MOV DI, AX
 MOV CX, DX
 MOV AX, CX
 MUL CX
 MUL CX
 ADD SI, AX
 MOV word SUM, SI
 CMP DI, O
 JNZ HESAPLA
 JZ COMPARE

COMPARE:
 MOV AX, word NUM
 CMP AX, SI
 JE ARMSTRONG
 JNE NOT_ARMSTRONG

NOT_ARMSTRONG:
 MOV AX, 0

ARMSTRONG:
 print reg
